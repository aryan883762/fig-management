{
  "extends": "eslint:recommended",
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "no-console": "warn",
    "no-extra-parens": "error",
    "indent": [
      "error",
      2
    ],
    "default-case": "error"
  },
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  }
}